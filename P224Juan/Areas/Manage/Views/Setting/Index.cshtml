@model Setting
@{
    ViewData["Title"] = "Index";
}

<div class="col-lg-6">
    <h1>Settings</h1>
</div>
<div class="row">
    <div class="col-lg-12">
        <table class="table table-dark">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Logo</th>
                    <th scope="col">Email</th>
                    <th scope="col">Phone</th>
                    <th scope="col">Address</th>
                    



                </tr>
            </thead>
            <tbody>
                <tr>
                    <th scope="row">1</th>
                    <td> <img src="~/assets/img/logo/@Model.Logo" alt="Alternate Text" /> </td>
                    <td>@Model.Email</td>
                    <td>@Model.Phone</td>
                    <td>@Model.Address</td>

                    <td>
                        <a class="btn btn-primary" asp-action="Detail">Detail</a>
                        <a class="btn btn-warning" asp-action="Update">Update</a>
                    </td>

                </tr>
               
            </tbody>
        </table>
    </div>
</div>

